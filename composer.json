{
  "name": "freyr/offer",
  "description": "description",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "mgiergielewicz",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": ">=8.4.0",
    "ext-json": "*",
    "freyr/identity": "^0.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "ergebnis/composer-normalize": "^2.45",
    "phpcompatibility/php-compatibility": "10.x-dev as 9.99.0",
    "phpcompatibility/phpcompatibility-symfony": "^1.2",
    "kubawerlos/php-cs-fixer-custom-fixers": "^3.7.2",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "symplify/easy-coding-standard": "^12.5"
  },
  "autoload": {
    "psr-4": {
      "Freyr\\Offer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freyr\\EventSourcing\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "ecs": "vendor/bin/ecs check --fix",
    "phpstan": "vendor/bin/phpstan --memory-limit=-1",
    "test": "vendor/bin/phpunit"
  }
}